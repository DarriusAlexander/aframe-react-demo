  "homepage": "https://luatnd.github.io/aframe-react-demo/",
  "description": "Demo (can use as boilerplate) for building virtual reality experiences with A-Frame and React with exist example and resolve some react and aframe production issue.",
  "repository": {
    "type": "git",
    "url": "https://github.com/luatnd/aframe-react-demo.git"
  },
  "keywords": [
    "aframe",
    "react",
    "vr",
    "a-frame",
    "mozvr",
    "reactvr",
    "aframe-react",
    "webvr",
    "typescript",
    "redux"
